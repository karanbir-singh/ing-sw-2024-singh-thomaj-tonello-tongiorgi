<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<StackPane fx:id="RootPane" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="it.polimi.ingsw.gc26.ui.gui.sceneControllers.LoginController">
   <children>
      <VBox fx:id="Background" alignment="CENTER" fillWidth="false" minHeight="300.0" minWidth="200.0" spacing="30.0">
         <children>
            <Label fx:id="status" text="Status" textAlignment="CENTER" visible="false">
               <font>
                  <Font size="18.0" />
               </font>
               <VBox.margin>
                  <Insets bottom="70.0" />
               </VBox.margin>
            </Label>
            <TextField fx:id="nicknameTXT" prefHeight="27.0" prefWidth="205.0" />
            <Button mnemonicParsing="false" onAction="#onClickButton" text="Login">
               <font>
                  <Font size="18.0" />
               </font>
            </Button>
         </children>
      </VBox>
   </children>
</StackPane>
